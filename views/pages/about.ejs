<%- contentFor('hero') %>
<section class="hero">
  <div class="hero-content">
    <h2>All about us!</h2>
    <h6>We're a super small team.</h6>

    <a href="/contact" class="btn btn-lg btn-primary">Drop us a line</a>
  </div>
</section>

<%- contentFor('body') %>
<div class="jumbotron text-center">
    <h1>The Team</h1>

    <div class="row">
        <% for (user of users) { %>
        <div class="col-sm-4"></div>
        <div class="col-sm-4">
            <h2><%= user.name %>
            <img src="<%= user.avatar %>" alt="" class="img-responsive">
        </div>
        <div class="col-sm-4"></div>
        <% } %>
    </div>
</div>